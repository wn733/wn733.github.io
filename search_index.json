,4,5]  [2,3,4,5,6]\n\nes6中遍历\n    let evens = [1,2,3,4,5];\n    let odds = evens.map( v => v+1 );\n    console.log(evens,odds);//[1,2,3,4,5]  [2,3,4,5,6]\n\nes5this的指向\n    var factory = function (){\n        this.a = 'a',\n        this.b = 'b',\n        this.c = {\n            a:'a+',\n            b:function(){\n                return this.a\n            }\n        }\n    }\n    console.log(new factory().c.b()); //a+\n\nthis的指向是该函数被调用的对象\nes6this的指向\n    var factory = function (){\n        this.a = 'a',\n        this.b = 'b',\n        this.c = {\n            a:'a+',\n            b:() => {\n                return this.a\n            }\n        }\n    }\n    console.log(new factory().c.b()); //a\n\n箭头函数中this的指向是定义时this的指向\n"},"doc/ES6/es6-默认参数.html":{"url":"doc/ES6/es6-默认参数.html","title":"es6-默认参数","keywords":"","body":"es6-默认参数\nes5中默认参数写法\n    function f(x,y,z){\n        if(y === undefined){\n            y = 7;\n        }\n        if(z === undefined){\n            z = 42;\n        }\n        return x+y+z\n    }\n    console.log(f(1));//50  1+7+42\n    console.log(f(1,3));//46 1+3+42\n\nes6中默认参数写法\n    function f(x,y=7,z=42){\n        return x+y+z\n    }\n    console.log(f(1));//50  1+7+42\n    console.log(f(1,3));//46 1+3+42\n\nes6中默认参数必选检查\n    function check(){\n        throw new Error(\"can`t be empty\")\n    }\n    function f(x=check(),y=7,z=42){\n        return x+y+z\n    }\n    console.log(f(1));//50\n    try{\n        f()\n    }catch(e){\n        console.log(e); //can`t be empty\n    }\n\nes5中可变参数\n    function f(){\n        var a = Array.prototype.slice.call(arguments);\n        var sum = 0;\n        a.forEach(function(i){\n            sum+=i*1\n        })\n        return sum\n    }\n    console.log(f(1,2,3,6)) //12\n\nes6中可变参数\n    function f(...a){\n        var sum = 0;\n        a.forEach(function(i){\n            sum+=i*1\n        })\n        return sum\n    }\n    console.log(f(1,2,3,6)) //12\n\nes5扩展运算符合并\n    var params = ['hello',true,7];\n    var other = [1,2].concat(params);\n    console.log(other); //[1,2,'hello',true,7]\n\nes6扩展运算符合并\n    var params = ['hello',true,7];\n    var other = [1,2,...params];\n    console.log(other); //[1,2,'hello',true,7]\n\n"},"doc/ES6/es6-对象代理.html":{"url":"doc/ES6/es6-对象代理.html","title":"es6-对象代理","keywords":"","body":"es6-对象代理\n"},"doc/Promise/":{"url":"doc/Promise/","title":"Promise入门","keywords":"","body":"Promise入门\n"},"doc/Promise/promise-1.html":{"url":"doc/Promise/promise-1.html","title":"promise-1","keywords":"","body":"promise-1\n"},"doc/Promise/promise-2.html":{"url":"doc/Promise/promise-2.html","title":"promise-2","keywords":"","body":"promise-2\n"},"doc/TypeScipt/":{"url":"doc/TypeScipt/","title":"TypeScript入门","keywords":"","body":"TypeScript入门\n"},"doc/TypeScipt/typescript-1.html":{"url":"doc/TypeScipt/typescript-1.html","title":"typescript-1","keywords":"","body":"typescript-1\n"},"doc/TypeScipt/typescript-2.html":{"url":"doc/TypeScipt/typescript-2.html","title":"typescript-2","keywords":"","body":"typescript-2\n"},"doc/node/":{"url":"doc/node/","title":"node.js调试入门","keywords":"","body":"node.js调试入门\n"},"doc/node/node-1.html":{"url":"doc/node/node-1.html","title":"node-1","keywords":"","body":"node-1\n"},"doc/node/node-2.html":{"url":"doc/node/node-2.html","title":"node-2","keywords":"","body":"node-2\n"},"doc/vue/":{"url":"doc/vue/","title":"vue.js入门","keywords":"","body":"vue.js入门\n"},"doc/vue/vue-1.html":{"url":"doc/vue/vue-1.html","title":"vue-1","keywords":"","body":"vue-1\n"},"doc/vue/vue-2.html":{"url":"doc/vue/vue-2.html","title":"vue-2","keywords":"","body":"vue-2\n"}}}